{{!
    @template block_myoverview/nav-year-selector

    This template renders year dropdown.

}}
{{#years}}
<div class="dropdown mb-1 me-1">
    <button id="yeardropdown" type="button" class="btn btn-outline-secondary dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" aria-label="{{#str}} year, theme_snap {{/str}}">
        <span data-active-item-text>
            {{#str}} year, theme_snap {{/str}}
        </span>
    </button>
    <ul class="dropdown-menu" role="menu" data-show-active-item data-skip-active-class="true" data-active-item-text aria-labelledby="yeardropdown">
        {{{years}}}
    </ul>
</div>
{{/years}}